{
  "name": "pagination-harvest",
  "type": "module",
  "version": "2.0.2",
  "description": "A blazing fast, parallelized, and small data harvester for paginated APIs. Supports Axios and custom fetchers, progress callbacks, and automatic retry.",
  "source": "./src/paginationHarvest.ts",   
  "main": "./dist/paginationHarvest.cjs",
  "module": "./dist/paginationHarvest.mjs",
  "unpkg": "./dist/paginationHarvest.umd.js", 
  "types": "./dist/paginationHarvest.d.ts",
  "exports": {
    ".": {
      "import": "./dist/paginationHarvest.mjs",
      "require": "./dist/paginationHarvest.cjs",
      "types": "./dist/paginationHarvest.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "microbundle"
  },
  "devDependencies": {
    "microbundle": "^0.15.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimsp4/pagination-harvest.git"
  },
  "keywords": [
    "pagination",
    "harvest",
    "fetch",
    "axios",
    "data",
    "api",
    "parallel",
    "typescript",
    "fetch all pages",
    "parallel API requests",
    "paginated API",
    "async pagination",
    "custom fetcher",
    "retry API requests",
    "concurrent fetch",
    "high performance fetch",
    "TypeScript library",
    "non-blocking API",
    "page-based harvesting",
    "progress callback",
    "error resilient fetch",
    "scalable data collection",
    "dynamic page limit",
    "API data pipeline",
    "lightweight pagination",
    "browser API fetch",
    "node.js pagination",
    "universal page fetcher",
    "streaming API data",
    "real-time progress",
    "fetch with retries",
    "page size control"
  ],
  "author": "dimszy",
  "license": "MIT"
}